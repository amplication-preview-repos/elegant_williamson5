datasource db {
  provider = "mongodb"
  url      = env("DB_URL")
}

generator client {
  provider = "prisma-client-js"
}

model WebScan {
  id         String   @id @default(auto()) @map("_id") @db.ObjectId
  createdAt  DateTime @default(now())
  updatedAt  DateTime @updatedAt
  url1       String?
  url2       String?
  scanResult String?
  uniqueId   String?
  url3       String?
}

model CloudScan {
  id         String   @id @default(auto()) @map("_id") @db.ObjectId
  createdAt  DateTime @default(now())
  updatedAt  DateTime @updatedAt
  accessKey  String?
  secretKey  String?
  scanResult String?
  uniqueId   String?
}
